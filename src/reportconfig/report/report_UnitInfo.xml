	<page id="unitInfo_lianwang" js="/webresources/script/validate.js">
		<report id="report_add_update" title="联网单位" type="form">
			<display labeltdwidth="100"  valuetdwidth="120" labelbgcolor="#f0f0f0" labelalign="center" valuealign="center">
				<col column="id" displaytype="hidden" ></col>
				<col column="UnitName" label="单位名称" ></col>
				<col column="UnitCode" label="单位编码" ></col>				
				<col column="NodeNum" label="总结点数" br="true" >
					<inputbox jsvalidate="isNotEmpty(#data#总结点数不能为空)"  styleproperty="style='width:95%'"></inputbox>
				</col>
				<col column="Coordinate" label="单位坐标" valuestyleproperty="colspan='2'">
					<inputbox jsvalidate="isNotEmpty(#data#单位坐标不能为空)" styleproperty="style='width:95%'"></inputbox>
				</col>
				<col column="UnitAddr" label="单位地址" valuestyleproperty="colspan='4'" br="true">
					<inputbox jsvalidate="isNotEmpty(#data#单位地址不能为空)" styleproperty="style='width:98%'"></inputbox>
				</col>
				<col column="HeadName" label="联系人姓名" >
					<inputbox jsvalidate="isNotEmpty(#data#联系人姓名不能为空)" styleproperty="style='width:95%'"></inputbox>
				</col>
				<col column="HeadTel" label="联系电话" >
					<inputbox jsvalidate="isNotEmpty(#data#联系电话不能为空)" styleproperty="style='width:95%'"></inputbox>
				</col>
				<col column="Organization" label="组织机构">
					<inputbox jsvalidate="isNotEmpty(#data#组织机构不能为空)" styleproperty="style='width:95%'"></inputbox>
				</col>
				<col column="EnterpriseType" label="企业类型" br="true">
					<inputbox jsvalidate="isNotEmpty(#data#企业类型不能为空)" styleproperty="style='width:95%'"></inputbox>
				</col>
				<col column="ContactsName" label="负责人姓名">
					<inputbox jsvalidate="isNotEmpty(#data#负责人姓名不能为空)" styleproperty="style='width:95%'"></inputbox>
				</col>
				<col column="ContactsTel" label="负责人联系电话">
					<inputbox jsvalidate="isNotEmpty(#data#负责人联系电话不能为空)" styleproperty="style='width:95%'"></inputbox>
				</col>
				<col column="ContactsMail" label="负责人联系邮箱" valuestyleproperty="colspan='2'" br="true">
					<inputbox jsvalidate="isNotEmpty(#data#负责人联系邮箱不能为空)" styleproperty="style='width:95%'"></inputbox>
				</col>
				<col column="JoinTime" label="入网时间" datatype="date{yyyy-MM-dd}">
					<inputbox type="datepicker" jsvalidate="isNotEmpty(#data#入网时间不能为空);isDate(#data#格式不对，必须为yyyy-MM-dd格式)"></inputbox>
				</col>
				<col column="UpdateTime" label="更新日期" datatype="date{yyyy-MM-dd}">
					<inputbox type="datepicker" jsvalidate="isNotEmpty(#data#更新日期不能为空);isDate(#data#格式不对，必须为yyyy-MM-dd格式)"></inputbox>
				</col>
				<col column="LegalPersonName" label="法人姓名">
					<inputbox jsvalidate="isNotEmpty(#data#法人姓名不能为空)" styleproperty="style='width:95%'"></inputbox>
				</col>
				<col column="LegalPersonTel" label="法人联系电话" br="true">
					<inputbox jsvalidate="isNotEmpty(#data#法人联系电话不能为空)" styleproperty="style='width:95%'"></inputbox>
				</col>
				<col column="MonitoringUnitId" label="所属监控单位" >
					<inputbox type="selectbox" jsvalidate="isNotEmpty(所属监控单位必须选择)" styleproperty="style='width:98%'">
								<option label="==请选择==" value="" ></option>
								<option dataset="@{select id,UnitName from UnitInfo where UnitType='监控单位'}" label="UnitName" value="id"></option>
					</inputbox>
				</col>
				<col column="CalledUnitId" label="接警单位" >
					<inputbox type="selectbox" jsvalidate="isNotEmpty(接警单位必须选择)" styleproperty="style='width:98%'">
						<option label="==请选择==" value="" ></option>
						<option dataset="@{select id,UnitName from UnitInfo where UnitType='接警单位'}" label="UnitName" value="id"></option>
					</inputbox>
				</col>
				<col column="RepairUnitId" label="维保单位" >
					<inputbox type="selectbox" jsvalidate="isNotEmpty(维保单位必须选择)" styleproperty="style='width:98%'">
						<option label="==请选择==" value="" ></option>
						<option dataset="@{select id,UnitName from UnitInfo where UnitType='维保单位'}" label="UnitName" value="id"></option>
					</inputbox>
				</col>
				<col column="EliminationUnitId" label="消检单位" br="true" >
					<inputbox type="selectbox" jsvalidate="isNotEmpty(消检单位必须选择)" styleproperty="style='width:98%'">
						<option label="==请选择==" value="" ></option>
						<option dataset="@{select id,UnitName from UnitInfo where UnitType='消检单位'}" label="UnitName" value="id"></option>
					</inputbox>
				</col>
			</display>
			<sql>
				<select>
					<value>
						<![CDATA[SELECT * FROM UnitInfo where {#condition#}]]>
					</value>
					<condition name="txtid" hidden="true">
					<value>
						<![CDATA[(id like '%#data#%' )]]>
					</value>
				</condition>
				</select>
				<!-- <insert>
		  		 	insert into UnitInfo (id=newid(),@{UnitName},@{UnitCode},@{Coordinate},UnitType='联网单位',
		  		 	@{UnitAddr},@{NodeNum},@{HeadName},@{HeadTel},@{Organization},@{EnterpriseType},IsRemove=0,
		  		 	@{ContactsName},@{ContactsTel},@{ContactsMail},@{JoinTime},@{UpdateTime},@{LegalPersonName},@{LegalPersonTel},
		  		 	@{MonitoringUnitId},@{CalledUnitId},@{RepairUnitId},@{EliminationUnitId});
			  	</insert> -->
			  	<update>
			  		update UnitInfo (@{UnitAddr},@{NodeNum},@{HeadName},@{HeadTel},@{MonitoringUnitId},@{CalledUnitId},@{RepairUnitId},@{EliminationUnitId}),
			  		@{Coordinate},@{Organization},@{EnterpriseType},@{ContactsName},@{ContactsTel},@{ContactsMail},@{JoinTime},@{UpdateTime},@{LegalPersonName},@{LegalPersonTel}
			  		where id = @{id};
			  	</update>
			</sql>
			<buttons>
				<button name="saveUnitInfo_lianwang" position="title" label="保存">
					saveEditableReportData({pageid:'unitInfo_lianwang',savingReportIds:[{reportid:'report_add_update',updatetype:'save'}]});
				</button>
			</buttons>
			<interceptor>
				<imports>
	  				<import>com.wabacus.util.*</import>
	  			</imports>
				<preaction>				
	  	 			rrequest.authorize("report_add_update",Consts.BUTTON_PART,"type{save}","display","false");
	  	 			rrequest.authorize("report_add_update",Consts.BUTTON_PART,"type{add}","display","false");
	  	 			rrequest.authorize("report_add_update",Consts.BUTTON_PART,"type{reset}","display","false");
				</preaction>
			</interceptor>
		</report>
		</page>
