	<page id="unitInfo"  js="/wabacusdemo/js/demo.js" interceptor="com.ccu.pageInterceptor.unitInfoPageInterceptor">
		<tabpanel id="tabpanel1"  margin="10px" titlestyle="2"  titlewidth="150px"  border="0">
		<report id="showUnitInfo_lianwang" parenttitle="联网单位"  title="单位基本信息列表" type="editablelist" rowselect="checkbox"
			interceptor="com.ccu.reportInterceptor.reportInterceptor">
			<display>
				<col column="id" displaytype="hidden"></col>
				<col column="UnitType" label="单位类型"  displaytype="hidden"></col>
				<col column="{sequence:1}" label="序号" width="80px"></col>
				<col column="{col-rowselect}" label="全选"  width="80px"></col>
				<col column="UnitCode" label="单位编码" ></col>
				<col column="UnitName" label="单位名称" ></col>			
				<col column="UnitAddr" label="单位地址" width="300px"></col>
				<col column="NodeNum" label="总结点数"  displaytype="hidden"></col>
				<col column="IsRemove" label="是否测试" ></col>
				<col column="HeadName" label="联系人" ></col>
				<col column="HeadTel" label="联系电话" ></col>
				<col column="{editablelist-edit}" label="编辑"  width="80"></col>
			</display>
			<sql>
				<select>
					<value>
						<![CDATA[SELECT * FROM UnitInfo where UnitType='联网单位' and {#condition#}]]>
					</value>
					<condition name="txtUnitCode" label="单位编码" left="10" labelposition="left" >
						<value>
							<![CDATA[(UnitCode like %#data#%)]]>
						</value>
						<inputbox>
					        <typeprompt count="15" callbackmethod="testTypePromptCallBack">
						       <promptcol label="UnitCode" title="单位编码" matchmode="anywhere"></promptcol>
						       <promptcol label="UnitName" title="单位名称"></promptcol>
						       <datasource>        
						       		<option dataset="@{select UnitCode,UnitName from UnitInfo where {#matchcondition#}}"></option>       
						       	</datasource>
					        </typeprompt>
					     </inputbox>
					</condition>
					<condition name="txtUnitName" label="单位名称" left="10" right="10" labelposition="left"> 
						<value>
							<![CDATA[(UnitName like %#data#%)]]>
						</value>
						<inputbox>
					        <typeprompt count="15" width="200" callbackmethod="testTypePromptCallBack">
							       <promptcol label="UnitName" title="单位名称" matchmode="anywhere"></promptcol>
							       <datasource>
								        <option dataset="@{select UnitName from	UnitInfo where {#matchcondition#}}"></option>
							       </datasource>
					        </typeprompt>
					     </inputbox>
					</condition>
				</select>
				<update pageurl="report{unitInfo_lianwang.report_add_update}" urlparams="txtid=@{id}"  popupparams="{title:'编辑单位资料',width:920,height:300}"/>
			</sql>
			<format>
				<value>
					IsRemove=IsRemove==null?"":IsRemove.trim();
					if(IsRemove.equals("0")) IsRemove="否";
					else if(IsRemove.equals("1")) IsRemove="是";
			  </value>
			</format>
			<buttons buttonspacing="5">
				<button name="addUnitInfo" label="添加" position="title">
					wx_winpage('ShowReport.wx?PAGEID=addUnitInfo_lianwang',{width:920,height:300,title:'添加单位'});
				</button>
				<button name="deleteUnitInfo" class="com.wabacus.system.buttons.ServerSQLActionButton"  position="title" label="删除" >
					delete dbo.UnitInfo where id = @{id};
				</button>
			</buttons>
		</report>
